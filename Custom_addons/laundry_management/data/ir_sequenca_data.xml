<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="ir_sequence_laundry" model="ir.sequence">
        <field name="name">Laundry Order Code</field>
        <field name="code">laundry.order</field>
        <field eval="4" name="padding"/>
        <field name="prefix">LO</field>
    </record>
    
    <record id="ir_sequence_contact_query" model="ir.sequence">
        <field name="name">Contact Query Code</field>
        <field name="code">laundry.contact.query</field>
        <field eval="4" name="padding"/>
        <field name="prefix">CQ</field>
    </record>
    
    <record id="ir_sequence_pickup_request" model="ir.sequence">
        <field name="name">Pickup Request Code</field>
        <field name="code">laundry.pickup.request</field>
        <field eval="4" name="padding"/>
        <field name="prefix">PR</field>
    </record>
</odoo>
